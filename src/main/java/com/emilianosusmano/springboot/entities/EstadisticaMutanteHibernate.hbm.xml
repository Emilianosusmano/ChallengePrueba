<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.emilianosusmano.springboot.entities.EstadisticaMutanteHibernate" table="estadistica_mutante_hibernate">
		<id name="idPersona" column="id_persona">
			<generator class="native"></generator>
		</id>
		<property name="dna" column="dna" type="java.lang.String"/>
		<property name="isMutant" column="is_mutant"/>
		<property name="cantCadenas" column="cant_cadenas"/>
	</class>
</hibernate-mapping>